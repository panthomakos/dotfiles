Config { font = "-*-Fixed-Bold-R-Normal-*-13-*-*-*-*-*-*-*"
, bgColor = "black"
, fgColor = "grey"
, position = Top
, lowerOnStart = True
, commands = [ Run Weather "KOAK" ["-t","<tempC>C","-L","18","-H","25","--normal","green","--high","red","--low","lightblue"] 36000
             , Run Network "wlp3s0" ["-L","0","-H","32","--normal","green","--high","red"] 10
             , Run Wireless "wlp3s0" ["-t","<essid>"] 10
             , Run Cpu ["-L","3","-H","50","--normal","green","--high","red"] 10
             , Run Com "/home/pan/.xmonad/vpn.sh" [] "vpn" 10
             , Run Memory ["-t","Mem: <usedratio>%"] 10
             , Run Date "%a %Y %m %d %H:%M:%S" "date" 10
             , Run Battery ["-t","<left>% / <timeleft>","-L","50","-H","75","-h","green","-n","yell","-l","red"] 10
             , Run Com "/home/pan/.xmonad/volume.sh" [] "vol" 10
             , Run Com "/home/pan/.xmonad/bright.sh" [] "bright" 10
             , Run CommandReader "~/pymodoro/pymodoro.py" "pomodoro"
             , Run StdinReader
             ]
, sepChar = "%"
, alignSep = "}{"
, template = "%pomodoro% | %cpu% | %memory% | %vpn% | %wlp3s0wi% | %wlp3s0% | %battery% | %StdinReader% }{ %vol% | %bright% | %KOAK% | <fc=#ee9a00>%date%</fc>"
}

