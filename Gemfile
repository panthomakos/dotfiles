source :rubygems

gem 'boom' # Terminal copy/paste utility.
gem 'lunchy' # Wrapper for launchctl.
gem 'powder' # Install and manage pow servers.
gem 'pry' # A better IRB.
gem 'ronn' # Markdown previews and man/html pages.
gem 'tmuxinator' # YAML based TMUX configuration.
gem 'zeus' # Distributed Ruby/Rails server.
